<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <geoip module="geoip">
                                        <title>GeoIP Config</title>
                                        <sort_order>99</sort_order>
                                    </geoip>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>   
    </acl>
</config>